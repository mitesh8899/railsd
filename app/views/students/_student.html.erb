<tr id="student_<%= student.id %>">
  <td><%= student.name %></td>
  <td><%= student.roll_no %></td>
  <td><%= link_to 'Edit', edit_student_path(student.id), method: :get, class: :"btn btn-outline-secondary", remote: :true %></td>
  <td><%= link_to 'Delete',student_path(student.id), method: :delete, class: :"btn btn-outline-danger", remote: :true %></td>
  <td><%= link_to 'Show', student_path(student.id), method: :get, class: :"btn btn-outline-primary",remote: :true %></td>
</tr>
<!DOCTYPE html>
<html>
  <head>
    <title>Demo</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div class="row">
        <div class="col-2">
          <div id="side"><br>
         
          </div>
          <div id="main">
            <%= @content_for_layout %>
          </div>
        </div>
        <div class="col-6">
          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>